<div class="row">
    <h5><%= restock.prodName %> </h5>
    <p id="prodID" data-itemID="<%= restock.prodID %>">itemID : <%= restock.prodID %> </p>
</div>
<div class="row">
    <div class="input-group input-group-outline my-3">
        <input type="number" class="form-control" id="prodQuantity" placeholder="Quantity">
    </div>
    <div class="input-group input-group-static mb-4">
        <label for="supplier" class="ms-0">Select Supplier</label>
        <select class="form-control" id="supplier">
            <% for( let i = 0; i < restock.supplierDetails.length; i++ ) { %>
            <% var element=restock.supplierDetails[i] %>
            <option value="<%= element.sup_id %>"><%= element.sup_name %></option>
            <% } %>
        </select>
    </div>
</div>