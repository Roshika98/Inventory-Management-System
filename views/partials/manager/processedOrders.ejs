<h3 class="title">Processed Orders</h3>
<!-- </div> -->
<!-- </div><br><br><br> -->

<div class="card ">
    <div class="card-header">
        <div class="col-lg-12">
            <div class="nav-wrapper">
                <ul class="nav nav-tabs nav-tabs-primary" role="tablist">
                    <li class="nav-item">
                        <a class="nav-link active " data-bs-toggle="tab" href="#home" role="tab" aria-selected="true">
                            Sales
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link " data-bs-toggle="tab" href="#menu1" role="tab" aria-selected="false">

                            Inventory
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="card-body">
        <div class="tab-content">
            <div id="home" class="tab-pane active">

                <% if (salesOrders.length==0) { %>
                <div class="row">
                    <div class="col-md-10 mt-4">
                        <div class="col-12">
                            <div class="card my-4">
                                <div class="card-header p-0 position-relative mt-n4 mx-3 z-index-2">
                                    <div class="bg-gradient-success shadow-primary border-radius-lg pt-4 pb-3">
                                        <h4 class="text-white text-capitalize ps-3">Processed Supplier Orders</h4>
                                    </div>
                                </div>
                                <div class="card-body px-0 pb-2">
                                    <h6 class="mb-1"> No Supplier Orders Available</h6>
                                </div>
                                <div class="card-footer">

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <% } else { %>
                <div class="row">
                    <div class="col-12">
                        <div class="table">
                            <table class="table">
                                <thead class="text-primary">
                                    <tr>
                                        <th>
                                            Order ID
                                        </th>
                                        <th>
                                            Order Date</th>
                                        <th>
                                            Payment</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    <% for( let i = 0; i < salesOrders.length; i++ ) { %>
                                    <% var element=salesOrders[i] %>
                                    <tr>
                                        <td>
                                            <h6 class="mb-0 text-sm"><%= element.order_no %></h6>
                                        </td>
                                        <td>

                                            <% var t=element.order_date %>
                                            <% var d=new Date(t) %>
                                            <%= d.toDateString()+' '+d.toLocaleTimeString() %>

                                        </td>
                                        <td>
                                            <p class="text-s text-success font-weight-bold mb-0">Rs
                                                <%= element.sub_total %>
                                            </p>
                                        </td>
                                        <!-- <td class="align-middle">
                                            <a href="javascript:;" class="text-secondary font-weight-bold text-xs"
                                                data-toggle="tooltip" data-original-title="Edit user">
                                                Edit
                                            </a>
                                        </td> -->
                                    </tr>
                                    <% } %>

                                </tbody>
                            </table>
                        </div>



                    </div>
                </div>
                <% } %>

            </div>
            <div id="menu1" class="tab-pane">


                <% if (inventoryOrders.length==0) { %>
                <div class="row">
                    <div class="col-md-10 mt-4">
                        <div class="col-12">

                            <!-- <div class="card-header p-0 position-relative mt-n4 mx-3 z-index-2">
                                        <div class="bg-gradient-success shadow-primary border-radius-lg pt-4 pb-3">
                                            <h4 class="text-white text-capitalize ps-3">Processed Supplier Orders</h4>
                                        </div>
                                    </div> -->

                            <h6 class="title"> No Supplier Orders Available</h6>


                        </div>
                    </div>
                </div>
                <% } else { %>
                <div class="row">
                    <div class="col-12">
                        <div class="table">
                            <table class="table">
                                <thead class="text-primary">
                                    <tr>
                                        <th>
                                            Order ID
                                        </th>
                                        <th>
                                            Order Date</th>
                                        <th>
                                            Payout</th>
                                        <!-- <th
                                            class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">
                                        </th> -->
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for( let i = 0; i < inventoryOrders.length; i++ ) { %>
                                    <% var element=inventoryOrders[i] %>
                                    <tr>
                                        <td>
                                            <h6 class="title"><%= element.order_no %></h6>
                                        </td>
                                        <td>
                                            <!-- <span class="text-secondary text-xs font-weight-bold"> -->
                                            <% var t=element.order_date %>
                                            <% var d=new Date(t) %>
                                            <%= d.toDateString()+' '+d.toLocaleTimeString() %>
                                            <!-- </span> -->
                                        </td>
                                        <td>
                                            <p class="text-s text-success font-weight-bold mb-0">Rs
                                                <%= element.sub_total %>
                                            </p>
                                        </td>
                                        <!-- <td class="align-middle">
                                            <a href="javascript:;" class="text-secondary font-weight-bold text-xs"
                                                data-toggle="tooltip" data-original-title="Edit user">
                                                Edit
                                            </a>
                                        </td> -->
                                    </tr>
                                    <% } %>

                                </tbody>
                            </table>
                        </div>



                    </div>
                </div>
                <% } %>



            </div>
        </div>
    </div>
</div>

<script>
    const pageIndicator = document.getElementById('orders');
    pageIndicator.classList.add('active');
</script>